{
  "dashboard": {
    "id": null,
    "title": "Live Streaming Platform - Performance Dashboard",
    "description": "Comprehensive monitoring dashboard for 1000+ concurrent users live streaming platform",
    "tags": ["livestream", "scalable", "performance", "websocket", "sfu"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "server_id",
          "type": "query",
          "query": "label_values(livestream_connections_total, server_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "room_id",
          "type": "query",
          "query": "label_values(livestream_concurrent_connections, room_id)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "ðŸš€ System Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Active Connections",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 500},
                {"color": "red", "value": 900}
              ]
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "sum(livestream_connections_total{server_id=~\"$server_id\"})",
            "legendFormat": "Total Connections",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Active Rooms",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "sum(livestream_rooms_active{server_id=~\"$server_id\"})",
            "legendFormat": "Active Rooms",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Messages/Second",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 5000}
              ]
            },
            "unit": "short"
          }
        },
        "targets": [
          {
            "expr": "sum(rate(livestream_messages_total{server_id=~\"$server_id\"}[1m]))",
            "legendFormat": "Messages/sec",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 1},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "avg(livestream_cache_hit_rate{cache_level=\"total\"})",
            "legendFormat": "Total Hit Rate",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "ðŸ“Š Connection Metrics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
        "collapsed": false
      },
      {
        "id": 7,
        "title": "Connections Over Time",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 10},
        "yAxes": [
          {"label": "Connections", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (server_id) (livestream_connections_total{server_id=~\"$server_id\"})",
            "legendFormat": "Server {{server_id}}",
            "refId": "A"
          },
          {
            "expr": "sum(livestream_connections_total{server_id=~\"$server_id\"})",
            "legendFormat": "Total",
            "refId": "B"
          }
        ]
      },
      {
        "id": 8,
        "title": "Connection Rate",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 10},
        "yAxes": [
          {"label": "Connections/sec", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (server_id) (rate(livestream_connections_per_second_total{server_id=~\"$server_id\"}[1m]))",
            "legendFormat": "Server {{server_id}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Concurrent Users per Room",
        "type": "graph",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 19},
        "yAxes": [
          {"label": "Users", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "livestream_concurrent_connections{room_id=~\"$room_id\"}",
            "legendFormat": "Room {{room_id}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "title": "ðŸ’¬ Message Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 28},
        "collapsed": false
      },
      {
        "id": 11,
        "title": "Message Latency",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 29},
        "yAxes": [
          {"label": "Latency (ms)", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(livestream_message_latency_seconds_bucket{server_id=~\"$server_id\"}[5m])) by (le)) * 1000",
            "legendFormat": "95th Percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(livestream_message_latency_seconds_bucket{server_id=~\"$server_id\"}[5m])) by (le)) * 1000",
            "legendFormat": "50th Percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(livestream_message_latency_seconds_bucket{server_id=~\"$server_id\"}[5m])) by (le)) * 1000",
            "legendFormat": "99th Percentile",
            "refId": "C"
          }
        ]
      },
      {
        "id": 12,
        "title": "Message Throughput by Type",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 29},
        "yAxes": [
          {"label": "Messages/sec", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (message_type) (rate(livestream_messages_total{server_id=~\"$server_id\"}[1m]))",
            "legendFormat": "{{message_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 13,
        "title": "ðŸ—„ï¸ Cache Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 38},
        "collapsed": false
      },
      {
        "id": 14,
        "title": "Cache Hit Rate by Level",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 39},
        "yAxes": [
          {"label": "Hit Rate (%)", "show": true, "min": 0, "max": 100},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "livestream_cache_hit_rate",
            "legendFormat": "{{cache_level}} Cache",
            "refId": "A"
          }
        ]
      },
      {
        "id": 15,
        "title": "Cache Operations",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 39},
        "yAxes": [
          {"label": "Operations/sec", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (operation, cache_level) (rate(livestream_cache_operations_total[1m]))",
            "legendFormat": "{{operation}} ({{cache_level}})",
            "refId": "A"
          }
        ]
      },
      {
        "id": 16,
        "title": "Cache Memory Usage",
        "type": "graph",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 48},
        "yAxes": [
          {"label": "Memory (MB)", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "livestream_cache_memory_bytes / 1024 / 1024",
            "legendFormat": "{{cache_level}} Cache Memory",
            "refId": "A"
          }
        ]
      },
      {
        "id": 17,
        "title": "ðŸŽ¥ Media Streaming (SFU)",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 57},
        "collapsed": false
      },
      {
        "id": 18,
        "title": "Active Producers/Consumers",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 58},
        "yAxes": [
          {"label": "Count", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (media_kind) (livestream_media_producers_active)",
            "legendFormat": "Producers ({{media_kind}})",
            "refId": "A"
          },
          {
            "expr": "sum by (media_kind) (livestream_media_consumers_active)",
            "legendFormat": "Consumers ({{media_kind}})",
            "refId": "B"
          }
        ]
      },
      {
        "id": 19,
        "title": "Media Bitrate",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 58},
        "yAxes": [
          {"label": "Bitrate (Mbps)", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (media_kind, direction) (livestream_media_bitrate_bps) / 1000000",
            "legendFormat": "{{media_kind}} ({{direction}})",
            "refId": "A"
          }
        ]
      },
      {
        "id": 20,
        "title": "Packet Loss & RTT",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 67},
        "yAxes": [
          {"label": "Packet Loss (%)", "show": true, "min": 0, "max": 10},
          {"label": "RTT (ms)", "show": true}
        ],
        "targets": [
          {
            "expr": "avg(livestream_media_packet_loss_rate) * 100",
            "legendFormat": "Avg Packet Loss",
            "refId": "A",
            "yAxis": 1
          },
          {
            "expr": "avg(livestream_media_rtt_seconds) * 1000",
            "legendFormat": "Avg RTT",
            "refId": "B",
            "yAxis": 2
          }
        ]
      },
      {
        "id": 21,
        "title": "Adaptive Quality Changes",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 67},
        "yAxes": [
          {"label": "Changes/min", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (reason) (rate(livestream_adaptive_quality_changes_total[1m]) * 60)",
            "legendFormat": "{{reason}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 22,
        "title": "ðŸ”§ System Resources",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 76},
        "collapsed": false
      },
      {
        "id": 23,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 9, "w": 8, "x": 0, "y": 77},
        "yAxes": [
          {"label": "CPU %", "show": true, "min": 0, "max": 100},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "livestream_cpu_usage_percent",
            "legendFormat": "CPU Usage",
            "refId": "A"
          }
        ]
      },
      {
        "id": 24,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 9, "w": 8, "x": 8, "y": 77},
        "yAxes": [
          {"label": "Memory (MB)", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "livestream_memory_usage_bytes{type=\"rss\"} / 1024 / 1024",
            "legendFormat": "RSS",
            "refId": "A"
          },
          {
            "expr": "livestream_memory_usage_bytes{type=\"heap_used\"} / 1024 / 1024",
            "legendFormat": "Heap Used",
            "refId": "B"
          },
          {
            "expr": "livestream_memory_usage_bytes{type=\"heap_total\"} / 1024 / 1024",
            "legendFormat": "Heap Total",
            "refId": "C"
          }
        ]
      },
      {
        "id": 25,
        "title": "Event Loop Lag",
        "type": "graph",
        "gridPos": {"h": 9, "w": 8, "x": 16, "y": 77},
        "yAxes": [
          {"label": "Lag (ms)", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "livestream_event_loop_lag_seconds * 1000",
            "legendFormat": "Event Loop Lag",
            "refId": "A"
          }
        ]
      },
      {
        "id": 26,
        "title": "ðŸ›¡ï¸ Security & Rate Limiting",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 86},
        "collapsed": false
      },
      {
        "id": 27,
        "title": "Rate Limit Status",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 87},
        "yAxes": [
          {"label": "Requests/sec", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (user_type) (rate(livestream_rate_limit_requests_total[1m]))",
            "legendFormat": "{{user_type}} Requests",
            "refId": "A"
          },
          {
            "expr": "sum by (user_type) (rate(livestream_rate_limit_blocked_total[1m]))",
            "legendFormat": "{{user_type}} Blocked",
            "refId": "B"
          }
        ]
      },
      {
        "id": 28,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 87},
        "yAxes": [
          {"label": "Errors/min", "show": true},
          {"show": false}
        ],
        "targets": [
          {
            "expr": "sum by (error_type, component) (rate(livestream_errors_total[1m]) * 60)",
            "legendFormat": "{{error_type}} ({{component}})",
            "refId": "A"
          }
        ]
      },
      {
        "id": 29,
        "title": "ðŸ“ˆ Performance Summary",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 96},
        "collapsed": false
      },
      {
        "id": 30,
        "title": "Response Time Distribution",
        "type": "heatmap",
        "gridPos": {"h": 9, "w": 24, "x": 0, "y": 97},
        "targets": [
          {
            "expr": "sum(rate(livestream_http_request_duration_seconds_bucket[5m])) by (le)",
            "format": "heatmap",
            "refId": "A"
          }
        ],
        "heatmap": {
          "xBucketSize": null,
          "yBucketSize": null,
          "yBucketBound": "auto"
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(livestream_build_info[5m]) > 0",
          "iconColor": "green",
          "titleFormat": "Deployment"
        }
      ]
    }
  }
}